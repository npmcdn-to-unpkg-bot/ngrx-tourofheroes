  "bugs": {
    "url": "https://github.com/pauleveritt/ngrx-tourofheroes/issues"
  },
  "homepage": "https://github.com/pauleveritt/ngrx-tourofheroes#readme",
  "dependencies": {
    "@ngrx/store": "^1.3.3",
    "angular2": "^2.0.0-beta.15",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "^0.19.26",
    "typescript": "^1.8.10",
    "zone.js": "^0.6.10"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
